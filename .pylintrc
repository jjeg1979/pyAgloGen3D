[MASTER]
docstring-min-length=10
disable=missing-function-docstring,
        missing-class-docstring,
        missing-module-docstring,
        too-few-public-methods,
        import-error,
        pointless-statement,
        unused-import,
        unused-expression,
[FORMAT]
good-names=i,j,x,y,id,p,c1,c2,c3